{
    "manifest_version": 2,
    "name": "Magnit",
    "version": "1.1.0",
    "description": "Download the largest magnet link on a javbook page!",
    "icons": {
        "48": "icons/th.png"
    },
    "background": {
        "scripts": [
            "background.bundle.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/th.png",
        "default_title": "Magn!t"
    },
    "permissions": [
        "*://*/*",
        "tabs",
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "browser_style": true,
        "open_in_tab": true
    }
}