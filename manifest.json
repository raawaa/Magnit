{
    "manifest_version": 2,
    "name": "Magnit",
    "version": "2.0.0",
    "description": "Download the largest magnet link on a javbook page!",
    "icons": {
        "48": "icons/icon.png",
        "96": "icons/icon@2x.png"
    },
    "background": {
        "scripts": [
            "helpers.js",
            "lodash.min.js",
            "default_selectors.js",
            "background.js"
        ]
    },
    "page_action": {
        "browser_style": true,
        "default_icon": "icons/th.png",
        "default_title": "magnit!",
        "default_popup": "popup/popup.html"
    },
    "browser_action": {
        "default_icon": "icons/th.png",
        "default_title": "Magn!t"
    },
    "permissions": [
        "*://*/*",
        "tabs",
        "storage",
        "clipboardWrite"
    ]
}