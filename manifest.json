{
    "manifest_version": 2,
    "name": "Magnit",
    "version": "2.0.0",
    "description": "Download the largest magnet link on a javbook page!",
    "icons": {
        "48": "icons/icon.png",
        "96": "icons/icon@2x.png"
    },
    "background": {
        "scripts": [
            "helpers.js",
            "lodash.min.js",
            "default_selectors.js",
            "background.js"
        ]
    },
    "page_action": {
        "browser_style": true,
        "default_icon": "icons/th.png",
        "default_title": "magnit!",
        "default_popup": "popup/popup.html"
    },
    "browser_action": {
        "default_icon": "icons/th.png",
        "default_title": "Magn!t"
    },
    "options_ui": {
        "page": "/options_page/options.html",
        "browser_style": true
    },
    "permissions": [
        "*://*/*",
        "<all_urls>",
        "tabs",
        "storage",
        "clipboardWrite"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "{11b0229f-80a1-467e-b9e4-25ca38d5d0a1}",
            "strict_min_version": "42.0"
        }
    }
}